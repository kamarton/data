{
    "name": "yiisoft/data",
    "type": "library",
    "description": "Data providers",
    "keywords": ["data provider"],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/data/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/data"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "yiisoft/arrays": "^3.0@dev"
    },
    "suggest": {
        "yiisoft/cache": "^dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "yiisoft/cache": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Data\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Data\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
